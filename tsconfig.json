{
  "compilerOptions": {
      "allowSyntheticDefaultImports": true,
      "noImplicitThis": true,
      "strict": true,
      "noImplicitReturns": true,
      "strictPropertyInitialization": true,
      "strictNullChecks": true,
      "moduleResolution": "node",
      "experimentalDecorators": true,
      "jsx": "react",
      "noUnusedParameters": true,
      "noUnusedLocals": true,
      "noImplicitAny": true,
      "target": "es5",
      "module": "es6",
      "declaration": false,
      "outDir": "./dist",
      "sourceMap": true,
      "downlevelIteration": true,
      "allowJs": true,
      "baseUrl": ".",
      "lib": [
          "dom",
          "es7"
      ],
      "paths": {
          "@/*": ["src/*"],
          "@@/*": ["src/.umi/*"],
      }
  },
  "exclude": [
      "node_modules",
      "lib",
      "es"
  ],
  "include": [
      "src/**/*",
      "declaration.d.ts",
      "tsconfig.json"
  ]
}